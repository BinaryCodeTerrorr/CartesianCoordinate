using System;

class CoordinateSystemProgram
{
  static void Main()
  {
    // Read input data
    decimal x = decimal.Parse(Console.ReadLine());
    decimal y = decimal.Parse(Console.ReadLine());
    
    // Solve the problem
    if ((x == 0) && (y == 0))
    {
      Console.WriteLine("0");
    }
    else if (y == 0)
    {
      Console.WriteLine("6");
    }
    else if (x == 0)
    {
      Console.WriteLine("5");
    }
    else if ((x > 0) && (y > 0))             
    {                 
      Console.WriteLine("1");             
    }else if ((x > 0) && (y < 0))             
    {                 
      Console.WriteLine("4");             
    }else if ((x < 0) && (y > 0))             
    {                 
      Console.WriteLine("2");             
    }else if ((x < 0) && (y < 0))             
    {                 
      Console.WriteLine("3");             
    }

  }
}
